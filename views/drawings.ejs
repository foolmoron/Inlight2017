<% var title = 'Drawings' %>
<% include header %>

<% drawings.forEach(drawing => { %>
    <p>
        <div><b><%= drawing.uuid %></b></div>
        <img src="<%= dir %><%= drawing.uuid %>.png" />
        <div style="word-break: break-all;"><%= JSON.stringify(drawing.json) %></div>
    </p>
<% }) %>

<% include footer %>